{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte",
				"semi": false,
				"useTabs": false
			}
		},
		{
			"files": "*.js;*.ts",
			"options": {
				"useTabs": false
			}
		}
	]
}
